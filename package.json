{
  "name": "taro-calendar-heatmap",
  "version": "0.3.1",
  "description": "A calendar heatmap component build by taro.",
  "author": "<phillzou@gmail.com>",
  "keywords": [
    "taro",
    "heatmap",
    "calendar",
    "小程序",
    "日历"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Mayandev/taro-calendar-heatmap.git"
  },
  "homepage": "https://github.com/Mayandev/taro-calendar-heatmap",
  "browser": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "source": "src/components/calendar-heat-map/index.tsx",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "clean": "rm -rf dist/ types/ lib/",
    "prepublish": "npm run clean && npm run build",
    "format": "prettier --write '**/*.{json,scss,ts,tsx,js}'",
    "pretty-quick": "pretty-quick --staged",
    "prepare": "husky install",
    "build": "yarn run build:rollup && yarn run build:lib",
    "build:lib": "tsc --project ./tsconfig.build.json --declaration --declarationDir types",
    "build:rollup": "rollup --config ./rollup.config.js",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.1.3",
    "@tarojs/react": "3.1.3",
    "@tarojs/runtime": "3.1.3",
    "@tarojs/taro": "3.1.3",
    "classnames": "^2.2.6",
    "d3-scale": "^3.2.3",
    "dayjs": "^1.10.4",
    "lodash": "^4.17.21",
    "react": "^16.10.0",
    "react-dom": "^16.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@tarojs/mini-runner": "3.1.3",
    "@tarojs/webpack-runner": "3.1.3",
    "@types/d3-scale": "^3.2.2",
    "@types/react": "^16.0.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-preset-taro": "3.1.3",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.1.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.2.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.23.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "rollup-plugin-visualizer": "^4.1.0",
    "stylelint": "9.3.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.0",
    "d3-time": "^2.0.0"
  }
}
